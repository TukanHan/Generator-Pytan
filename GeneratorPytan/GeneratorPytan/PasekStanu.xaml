<UserControl x:Class="GeneratorPytan.PasekStanu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GeneratorPytan"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="750">
    <Grid>
        <StatusBar x:Name="statusBar">
            <DockPanel Width="{Binding ActualWidth, ElementName=statusBar, Mode=OneWay}" >
                <StackPanel Name="stackInformacji" Orientation="Horizontal">
                </StackPanel>

                <StackPanel Name="stackStanu" HorizontalAlignment="Right" Orientation="Horizontal" Margin="10,0">
                </StackPanel>
            </DockPanel>
        </StatusBar>
    </Grid>
</UserControl>
